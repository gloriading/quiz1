<%- include('../partials/header') %>


<div>
  <div class="row">
    <div class="col-sm-3">
        <div class="card border-secondary mb-1 mt-4">
          <div class="card-body">
            <h5>Trending Clucks</h5>
            <% for (let key in frequency) { %>
            <%= key %> • <%= frequency[key] %> <br>
            <% } %>
          </div>
        </div>
    </div>
    <div class="col-sm-9">
      <% for (let list of lists) { %>
            <div class="card border-secondary mb-2 mt-4">
              <div class="card-body">
                <div class="row">
                  <div class = "col-sm-2">
                  <img style="width:50px;" src="https://s-media-cache-ak0.pinimg.com/originals/b8/25/65/b82565b747a49fa28ecb6bc450bfddf9.png"/>
                  </div>
                  <div class = "col-sm-10">
                   <h6><%= list.username %>  •  <%= time_ago( list.created_at) %> </h6>
                   <p><%= list.content %></p>
                   <img style="height:200px;" src="/<%= list.image_url %>"/>


                  </div>
                </div>
              </div>
            </div>

      <% } %>

    </div>
  </div>
</div>




<%- include('../partials/footer') %>
