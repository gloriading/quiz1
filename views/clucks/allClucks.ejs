<%- include('../partials/header') %>


<div>
  <% for (let list of lists) { %>


        <div class="card border-secondary mb-3 mt-4">
          <div class="card-body">
            <div class="row">
              <div class = "col-sm-2">
              <img style="width:50px;" src="https://s-media-cache-ak0.pinimg.com/originals/b8/25/65/b82565b747a49fa28ecb6bc450bfddf9.png"/>
              </div>
              <div class = "col-sm-10">
               <h6><%= list.username %> | <%= list.created_at %></h6>
               <p><%= list.content %></p>
               <img style="height:200px;" src="<%= list.image_url %>"/>


              </div>
            </div>
          </div>
        </div>

  <% } %>
</div>




<%- include('../partials/footer') %>
